Package: cdrs
Title: Functions for the California Delta Residents Survey Data
Version: 0.0.0.9000
Authors@R: 
    person(given = "Kenji", 
    family = "Tomari", 
    email = "ktomari@ucdavis.edu", 
    role = c("aut", "cre"),
